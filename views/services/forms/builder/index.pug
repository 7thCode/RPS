extends ../../../backoffice_layout

block content
    section.content-header
        .h6 Builder
        ol.breadcrumb.hidden-xs.hidden-sm
            li
                a.text-gray(href='/backoffice')
                    i.fa.fa-dashboard
                    |  Home
    section.content(ng-controller="FormBuilderController")
        include partial/navbar
        .row#draggable_area
            .col-xs-3.col-sm-3
                include partial/elements
            .col-xs-9.col-sm-9
                include partial/form
        div(ng-show="opened")
                include partial/pallete